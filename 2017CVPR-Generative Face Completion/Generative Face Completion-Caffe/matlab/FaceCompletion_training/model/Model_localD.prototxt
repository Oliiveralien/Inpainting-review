###
layer {
  name: "conv5"           
  type: "Convolution"
  bottom: "conv4_2_relu"
  top: "conv5"
  convolution_param {
    num_output: 1
    kernel_size: 4
    weight_filler {
      type: "gaussian"
      std: 0.02
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}
layer {
  name: "sigmoid"
  type: "Sigmoid"
  bottom: "conv5"
  top: "output"
  loss_weight: 1.0
}
