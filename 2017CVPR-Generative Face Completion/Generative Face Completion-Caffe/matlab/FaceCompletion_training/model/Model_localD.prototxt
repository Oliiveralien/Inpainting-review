  top: "conv3_2_relu"
  relu_param {
    negative_slope: 0.2
  }
}
###
layer {
  name: "conv4_1"           
  type: "Convolution"
  bottom: "conv3_2_relu"
  top: "conv4_1"
  convolution_param {
    num_output: 512
    pad: 1
    kernel_size: 3
    weight_filler {
      type: "gaussian"
      std: 0.02
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}
layer {
  name: "relu4_1"
  type: "ReLU"
  bottom: "conv4_1"
  top: "conv4_1_relu"
  relu_param {
