{
    "seed": 2020,
    "save_dir": "release_model/",
    "data_loader": {
        "name": "dtd", 
        "zip_root": "../datazip",
        "flist_root": "../flist",
        "mask": "square",
        "extend": 20,
        "w": 256,
        "h": 256
    },
    "losses": {
        "gan_type": "nsgan",
        "hole_weight": 6,
        "valid_weight": 1,
        "pyramid_weight": 0.5,
        "adversarial_weight": 0.1
    },
    "trainer": {
        "type": "Adam",
        "lr": 1e-4,
        "d2glr": 1, 
        "beta1": 0.5,
        "beta2": 0.999,
        "verbosity": 2,
        "iterations": 33e4,
        "save_freq": 1e4,
        "valid_freq": 1e4,
        "niter": 10e4,
        "niter_steady": 31e4,
        "batch_size": 16,
        "num_workers": 4,
        "log_step": 100
    }
}
